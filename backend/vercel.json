{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "bun.lockb",
            "bunfig.toml"
          ]
        }
      }
    ],
    "buildCommand": "bun install && bun run build",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  